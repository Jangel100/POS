
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>


    td {
        font-size: 14px;
        font-weight: bold;
    }
</style>
<div class="container-fluid">
    <div class="card">
    </div>
    <div class="card-header" style="padding: 0.25rem 0.25rem !important;">
        <span class="float-center">
            <strong>
                Re- Reimpresión de Comprobantes
            </strong>
        </span>
    </div>
    <div class="card-body">
        <div class="form-group row p-2">
            <input type="hidden" name="txtidClienteConsulta" id="txtidClienteConsulta" />
            <label class="col-form-label">Cliente</label>
            <input id="txtClienteConsult" class="form-control col-md-2" autocomplete="on" name="txtClienteConsult" placeholder="Seleccione Cliente" />
            <label class="col-form-label">Periodo</label>
            <div class="col-md-2">
                <select id="cmbPeriodoConsult" class="form-control" name="cmbPeriodoConsult">
                    <option>Seleccione Periodo</option>
                </select>
            </div>

            <label class="col-form-label">Dia</label>
            <div class="col-md-2">
                <select id="cmbDiaConsult" class="form-control" name="cmbDiaConsult">
                    <option>Seleccione un dia</option>
                </select>
            </div>

            <label class="col-form-label">Folio</label>
            <div class="col-md-2">
                <select id="cmbFolioConsult" class="form-control" name="fFolioConsult">
                    <option>Seleccione folio</option>
                </select>
            </div>
            <div class="col-md-2">
                <input type="button" class="btn btn-light" name="btnNuevoFiltro" value="Nuevo filtro" />
            </div>
        </div>
        <div class="form-group row">
            <label class="col-form-label">Pedido</label>
            <div class="col-md-2">
                <input id="txtPedidoConsult" class="form-control" />
            </div>
            <input type="button" id="btnPedidoConsult" class="btn btn-primary" value="Buscar" />
        </div>
    </div>
    <table id="tblConsultaPedidos" class="table table-sm" style="width:100%">
        <thead>

        </thead>
        <tbody></tbody>
    </table>
    <div class="card-body">

        <br />
        <br />
        <br />
    </div>
    <div class="card-footer" style="padding: 0.0rem 1.25rem !important;">

    </div>
</div>
